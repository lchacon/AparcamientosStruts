"-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" 
"http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">

<struts-config>
	<form-beans> <form-bean name="lstAparcamientoForm" type="es.open4job.web.form.AparcamientoMotosForm" 
		/> </form-beans> 

	<action-mappings>
